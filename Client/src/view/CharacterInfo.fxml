<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="mainStage" alignment="TOP_CENTER" prefHeight="400.0" prefWidth="1000.0" styleClass="background" stylesheets="@../CSS/Common.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CharacterInfoController">
    <HBox alignment="CENTER" prefWidth="1000.0">
        <children>
            <HBox prefHeight="26.0" prefWidth="500.0">
                <children>
                    <Label fx:id="mainLabel" text="gameName" textFill="WHITE">
                        <font>
                            <Font size="25.0" />
                        </font></Label>
                </children></HBox>
            <HBox alignment="CENTER_RIGHT" prefWidth="500.0">
                <children>
                    <Button onAction="#onBackBtnPress" text="Back">
                    </Button>
                </children></HBox>
        </children>
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </VBox.margin></HBox>
    <HBox>
        <Label fx:id="descLabel" text="description" textFill="WHITE" wrapText="true">
            <font>
                <Font size="17.0" />
            </font></Label>
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
    </HBox>
    <HBox fx:id="characterBox">

    </HBox>
</VBox>
